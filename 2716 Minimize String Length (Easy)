class Solution(object):
    def minimizedStringLength(self, s):
        """
        :type s: str
        :rtype: int
        """
        dic = {}
        for i in s:
            if i not in dic:
                dic[i] = 1
            else:
                dic[i] += 1
        count = 0
        for i in dic:
            if dic[i] % 2 != 0:
                count +=1
            else:
                count += 1
        return count
        
